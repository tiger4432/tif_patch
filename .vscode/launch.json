{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:5500",
      "webRoot": "${workspaceFolder}",
      "file": "${workspaceFolder}/index.html"
    },
    {
      "name": "Open with Live Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.html",
      "console": "integratedTerminal"
    }
  ]
}